{
  "presets": ["es2015", "es2016", "es2017", "react", "stage-1", "flow"],
  "plugins": [
    ["import", { libraryName: "antd", style: "css" }],
    [
      "transform-decorators-legacy",
      "transform-async-to-generator"
    ],
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
