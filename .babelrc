{
  "presets": ["es2015", "es2016", "es2017", "react", "stage-1"],
  "plugins": [
    ["transform-decorators-legacy", "transform-function-bind", "transform-async-to-generator", "react-hot-loader/babel"],
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
